    <listener-class>com.briup.estore.web.lisenter.IndexLisenter</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>ViewServlet</display-name>
    <servlet-name>ViewServlet</servlet-name>
    <servlet-class>com.briup.estore.web.servlet.ViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
